{
  "name": "eonx-com/eoneopay-migrationphpsdk",
  "description": "PHP SDK for migrating users to eoneopay v2",
  "keywords": [
    "eoneopay",
    "sdk",
    "migration"
  ],
  "license": "BSD-3-Clause",
  "type": "library",
  "require": {
    "php": ">=7.1",
    "eonx-com/eoneopay-phpsdk": "^v1.0@beta",
    "eonx-com/sdkblueprint": "^1.0@beta",
    "eonx-com/utils": "^0.2",
    "ext-json": "*"
  },
  "require-dev": {
    "eonx-com/standards": "^0.2",
    "friendsofphp/php-cs-fixer": "^2.9",
    "indigophp/doctrine-annotation-autoload": "^0.1.0",
    "laravel/lumen-framework": "^5.5",
    "phpmd/phpmd": "^2.6",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11",
    "phpunit/phpunit": "^7.0",
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^4.0",
    "squizlabs/php_codesniffer": "3.*",
    "vlucas/phpdotenv": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "EoneoPay\\MigrationPhpSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\EoneoPay\\MigrationPhpSdk\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true
  }
}
